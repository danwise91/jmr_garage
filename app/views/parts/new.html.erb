<%= form_for @part, html: { multipart: true } do |f| %>
<%= f.label :name %>
<%= f.text_field :name %><br>


<%= f.label :description %>
<%= f.text_field :description%><br>

<%= f.label :category_id%>
<%= f.collection_select :category_id, Category.all, :id, :name%><br>

<%= f.label :discount%>
<%= f.number_field :discount%>%<br>

<%= f.label :price%>
<%= f.text_field :price%><br>

<div class="field">
  <%= f.label :image %><br>
  <%= f.file_field :image %>
</div><br>
<%=f.submit %>
<%end%>
